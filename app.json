{
  "expo": {
    "name": "Movies app",
    "slug": "Movies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.opoku.moviesapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.opoku.moviesapp",
      "versionCode": 1,
      "enableDangerousExperimentalLeanBuilds": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
